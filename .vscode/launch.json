    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Sudoku",
            "type": "debugpy",
            "request": "launch",
            "program": "Sudoku.py",
            "console": "integratedTerminal",
            "args": "-file .\\SudokuPuzzles\\sudoku-puzzle1.txt"
        }
    ]
}